{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "no-label-var": 0,
    "no-var": 0,
    "no-console": 0,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "after-used",
        "exportas": "^ignored",
        "exports":"^ignored",
        "Reflect":"^ignored"
      }
    ],
    "no-undef": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-func-assign": 0,
    "no-empty":0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential"
  ]
}